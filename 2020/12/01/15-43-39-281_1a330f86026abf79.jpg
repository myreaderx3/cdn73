        _xhr.open('GET', relatedposts_url, true);
        _xhr.setRequestHeader('crossOrigin', 'anonymous');
        _xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
        _xhr.onload = function() {

            _renderRelatedPosts(_xhr.response);
        }
